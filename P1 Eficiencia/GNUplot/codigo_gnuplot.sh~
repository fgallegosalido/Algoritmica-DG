

gnuplot> set title "Ejercicio 1 - Burbuja"      // Poner título a la gráfica
gnuplot> plot "tiempos_ej1.dat"                 // Crear gráfico
gnuplot> set xlabel "Repeticiones"              // Renombrar el eje de abscisas
gnuplot> set ylabel "Tiempo(s)"                 // Renombrar el eje de ordenadas
gnuplot> q                                      // Salir

gnuplot> f(x)=a*x*x+b*x+c
gnuplot> fit f(x) "tiempos_ej1.dat" via a, b,c
